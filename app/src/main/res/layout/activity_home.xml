<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <Button
        android:id="@+id/btn_alertSlider_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alert Slider"/>
    <Button
        android:id="@+id/btn_asyncTask_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Asynctask"/>

    <Button
        android:id="@+id/btn_collapsing_toolbar_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Collapsing Toolbar"/>
    <Button

        android:id="@+id/btn_singleton_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Singleton"/>
    <Button
        android:id="@+id/btn_navigation_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Navigation"/>
    <Button
        android:id="@+id/btn_list_view_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List View"/>
    <Button
        android:id="@+id/btn_slider_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Slider"/>
    <Button
        android:id="@+id/btn_tab_layout_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tab Layout"/>
    <Button

        android:id="@+id/btn_camera_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Camera Intent"/>
    <Button

        android:id="@+id/btn_button_display_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button Display"/>
    <Button
        android:id="@+id/btn_register_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register "/>
    <Button
        android:id="@+id/btn_login_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"/>

    <Button
        android:id="@+id/btn_main_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Main"/>

</TableLayout>